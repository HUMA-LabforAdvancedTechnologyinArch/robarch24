                    <item name="CodeInput" type_name="gh_string" type_code="10">"""
Load robot directly from ROS.

COMPAS FAB v0.27.0
"""
from mobile_robot_control.mobile_robot import MobileRobot
from mobile_robot_control.mobile_robot_client import MobileRobotClient

from compas_ghpython.artists import RobotModelArtist
from ghpythonlib.componentbase import executingcomponent as component
from scriptcontext import sticky as st

from compas_fab.ghpython.components import create_id


class ROSRobot(component):
    def RunScript(self, ros_client, prefix, load, connect):
        key = create_id(self, 'robot')

        if ros_client and ros_client.is_connected and load:
            # Load URDF from ROS
            robot = ros_client.load_robot(
                    load_geometry=True, 
                    urdf_param_name= '{}/robot_description'.format(prefix),
                    srdf_param_name= '{}/robot_description_semantic'.format(prefix),
                    precision='12f')
            robot.artist = RobotModelArtist(robot.model)
            mobile_robot = MobileRobot(model=robot.model, artist=robot.artist, semantics=robot.semantics, client=None)
            st[key] = mobile_robot
        
        if ros_client and ros_client.is_connected and connect:
            # client sometimes need to be restarted, without needing to reload geometry
            mobile_robot = st.get(key, None)
            if mobile_robot:  
                mobile_robot.mobile_client = MobileRobotClient(ros_client)
                mobile_robot.client = ros_client
        
        mobile_robot = st.get(key, None)
        return mobile_robot


