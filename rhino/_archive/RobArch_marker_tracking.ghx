                    <item name="CodeInput" type_name="gh_string" type_code="10">from scriptcontext import sticky as st
from compas.geometry import Transformation, Frame, Point
from compas.artists import Artist

# fixed_marker_plane = fixed_marker_planes[fix_to_marker]
fixed_marker_plane = fixed_marker_0_plane
fixed_marker_frame = Frame(fixed_marker_plane.Origin, fixed_marker_plane.XAxis, fixed_marker_plane.YAxis)

if robot.mobile_client.marker_frames.get(robot_pose):
    marker_pose = robot.mobile_client.marker_frames[robot_pose][fix_to_marker]
    
    MCF_in_RCF = marker_pose #measured MCF in RCF 
    MCF_in_BCF = MCF_in_RCF.transformed(robot.transformation_RCF_BCF()) #measured MCF in BCF
        
    BCF_in_MCF = Frame.from_transformation(Transformation.from_frame(MCF_in_BCF).inverted()) #BCF in measured MCF
        
    transformation_FMCF_WCF = Transformation.from_change_of_basis(fixed_marker_frame, Frame.worldXY()) #T from fixed MCF to WCF
    BCF_in_WCF = BCF_in_MCF.transformed(transformation_FMCF_WCF) #BCF in WCF
        
    print BCF_in_WCF
        
    #mean_base_frame = ...
    base_frame = BCF_in_WCF</item>
