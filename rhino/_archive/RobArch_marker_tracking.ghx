from ur_fabrication_control.kinematics.ur_kinematics import forward_kinematics
from ur_fabrication_control.kinematics.ur_params import ur_params



if robot and configuration:
    robot.update(configuration, visual=show_visual, collision=show_collision)
    

    if show_visual:
        visual = robot.artist.draw_visual()
        
        # Warning!!! Check the direction!
        visual = ghcomp.Rotate(visual, math.pi, rg.Plane.WorldXY)[0]
        
        robot_BCF = draw_frame(robot.BCF)
        init_robot_plane = rg.Plane.WorldXY
        visual = ghcomp.Orient(visual, init_robot_plane, robot_BCF)[0]
    
    if show_collision:
        collision = robot.artist.draw_collision()
    
    frames = robot.transformed_frames(configuration, group)
    
    if show_base_frame:
        BCF = robot.BCF
        
    if show_robot_frame:
        RCF = robot.RCF
        
    
    if show_end_effector_frame:    
        print robot
        ee_frame = robot.forward_kinematics(configuration, group)
        ee_frame = ee_frame.transformed(Rotation.from_axis_and_angle(Vector.Zaxis(), math.pi))
        print(ee_frame)
        print ee_frame
        if robot.attached_tool:
            tool0_frame = robot.from_tcf_to_t0cf([ee_frame])
        print("End-effector", ee_frame)
    
    if not show_frames:
        frames = None

    frame = robot.forward_kinematics(configuration, options=dict(solver='model'))
    
    if show_acm: # attached collision meshes
        M = []
        for acm in attached_collision_meshes:
            frame = robot.forward_kinematics(configuration, options=dict(solver='model', link_name=acm.link_name))
            T = Transformation.from_frame_to_frame(Frame.worldXY(), frame)
            mesh = acm.collision_mesh.mesh.transformed(T)
            M.append(MeshArtist(mesh).draw())
        attached_meshes = M




