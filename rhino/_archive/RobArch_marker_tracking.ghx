                    <item name="CodeInput" type_name="gh_string" type_code="10">from compas.geometry import Point, Quaternion, Frame
import json

reference_frame = "base"

def _receive_marker_frame_callback(message):
    pose_point = Point(message['translation']['x'], message['translation']['y'], message['translation']['z'])
    pose_quaternion = Quaternion(message['rotation']['w'], message['rotation']['x'], message['rotation']['y'], message['rotation']['z'])
    pose_frame = Frame.from_quaternion(pose_quaternion, pose_point)
    if not robot.mobile_client.marker_frames.get(robot_pose):
        robot.mobile_client.marker_frames[robot_pose] = {}
    robot.mobile_client.marker_frames[robot_pose][target_frame] = pose_frame
    print(pose_frame)

if subscribe:
    robot.mobile_client.tf_subscribe(target_frame, reference_frame, _receive_marker_frame_callback)

if unsubscribe:
    robot.mobile_client.tf_unsubscribe(target_frame, reference_frame, _receive_marker_frame_callback)

if export:
    with open(PATH, "w") as outfile:
        marker_data = {}
        for pose, markers in robot.mobile_client.marker_frames.items():
            marker_data[pose] = {}
            for marker, frame in markers.items():
                marker_data[pose][marker] = frame.to_data()
        json.dump(marker_data, outfile)

if clean_all:
    robot.mobile_client.marker_frames = {}

dict = robot.mobile_client.marker_frames
    
print "tf_clients: ", robot.mobile_client.tf_clients
print robot.mobile_client.marker_frames</item>
